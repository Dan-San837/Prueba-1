public interface IMetodoPago  {
    boolean confirmarPago();
    boolean modificarPrecio();
}
public class MetodoPagoEfectivo implements MetodoPago {
    private String efectivo;

    @Override
    public boolean confirmarPago() {
        // Implementación
        return true;
    }

    @Override
    public boolean modificarPrecio() {
        // Implementación
        return true;
    }

    // Getters y Setters
}

public class MetodoPagoTransferencia implements MetodoPago {
    private String transferencia;

    @Override
    public boolean confirmarPago() {
        // Implementación
        return true;
    }

    @Override
    public boolean modificarPrecio() {
        // Implementación
        return true;
    }

    // Getters y Setters
}

